[font]
size = 13

[font.bold]
family = "JetBrainsMono Nerd Font Regular"
style = "Bold"

[font.bold_italic]
family = "JetBrainsMono Nerd Font Regular"
style = "Bold Italic"

[font.italic]
family = "JetBrainsMono Nerd Font Regular"
style = "Italic"

[font.normal]
family = "JetBrainsMono Nerd Font Regular"
style = "Regular"

[font.offset]
x = 0
y = 0

[window]
decorations = "none"
opacity = 0.5

[window.dimensions]
columns = 120
lines = 50

[window.padding]
x = 10
y = 2
